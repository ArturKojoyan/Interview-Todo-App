generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:123456@localhost:3306/todoapp"
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  todos    Todo[]
}

model Todo {
  id        Int     @id @default(autoincrement())
  title     String
  completed Boolean @default(false)
  user      User    @relation(fields: [userId], references: [id])
  userId    Int
}
